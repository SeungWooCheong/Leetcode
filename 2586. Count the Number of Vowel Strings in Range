class Solution:
    def vowelStrings(self, words: List[str], left: int, right: int) -> int:
        num = 0
        for j in range(left, right+1):
            if (words[j][0] == "a" or words[j][0] == "e" or words[j][0] == "i" or words[j][0] == "o" or words[j][0] == "u")and (words[j][-1] == "a" or words[j][-1] == "e" or words[j][-1] == "i" or words[j][-1] == "o" or words[j][-1] == "u"):
                num += 1
            else:
                pass
        return num
        
