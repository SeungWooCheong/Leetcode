class Solution:
    def returnToBoundaryCount(self, nums: List[int]) -> int:
        num = 0
        if len(nums) == 1:
            return 0
        else:
            for i in range(1,len(nums)+1):
                if sum(nums[0:i]) == 0:
                    num += 1
                else:
                    pass
        return num
