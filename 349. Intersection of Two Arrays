class Solution:
    def intersection(self, nums1: List[int], nums2: List[int]) -> List[int]:
        intersect = []
        dic2 = {}
        for n2 in nums2:
            dic2[n2] = 1
        for d2 in dic2:
            if d2 in nums1:
                intersect.append(d2)
            else:
                pass
        return intersect
